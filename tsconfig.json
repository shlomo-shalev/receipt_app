{
    "compilerOptions": {
        "module": "esnext",
        "jsx": "react",
        "target": "es2017",
        "forceConsistentCasingInFileNames": false,
        "strict": false,
        "esModuleInterop": true,
        "moduleResolution": "node", 
        "baseUrl": ".", 
        "paths": { 
            "*/__DOM_DRIVER__": ["./*/web", "./*/reactnative"],
            "react": [ "./node_modules/@types/react" ]
        }
    }
}